<Window x:Class="CreateCharWpf.Inventory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CreateCharWpf"
        mc:Ignorable="d"
        Title="Inventory" Height="200" Width="400">
    <Grid>
        <TextBox x:Name="Helmet" HorizontalAlignment="Center" Margin="0,20,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Chestplate" HorizontalAlignment="Center" Margin="0,74,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120" Height="18"/>
        <TextBox x:Name="Weapon" HorizontalAlignment="Center" Margin="0,123,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBlock HorizontalAlignment="Left" Margin="88,21,0,0" TextWrapping="Wrap" Text="Helmet" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="79,75,0,0" TextWrapping="Wrap" Text="Chestplate" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="91,124,0,0" TextWrapping="Wrap" Text="Weapon" VerticalAlignment="Top"/>


    </Grid>
</Window>
